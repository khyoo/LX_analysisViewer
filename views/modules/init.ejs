    // console.log(geojson);
    var filename = 'poi.geojson';

    var createUrl = '/download/create';
    var downloadUrl = '/download';

    $.ajax({
      type: "POST",
      url: createUrl,
      data: {
        filename: filename,
        source: geojson
      },
      dataType: "json",
      beforeSend: function () {
        $('#div_load_image').show();
      },
      success: function (data) {
        $('#div_load_image').hide();

        location.href = downloadUrl + '/' + filename;
      },
